{
    "name": "laravel-admin-ext/uploader",
    "description": "一个可以灵活配置的上传组件，支持自定义样式和自动缩放布局",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/ledtest/uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "ledtest",
            "email": "3130836035@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Uploader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Uploader\\UploaderServiceProvider"
            ]
        }
    }
}
